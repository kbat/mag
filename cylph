c tmesh
c rmesh121:p flux
c cora121 -100.0 100.0
c corb121 -50.0 49i 50.0
c corc121 -100.0 49i 100.0
c endmd
c c
mode n p e |
c
imp:n,p,e,| 0 1 2r
phys:n EMAX
cut:n j 0.0
phys:p EMAX 2j 1
c lca 2 1 1 0023 1 1 0 1 0 0
c lea 1 4 1 0 1 0 0 1
print
prdmp 2j 1
c stop nps 1
stop ctme 120
